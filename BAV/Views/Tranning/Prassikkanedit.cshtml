@model BAV.Models.Ansartranning
@{
    Layout = null;
}


<link rel="stylesheet" href="//code.jquery.com/ui/1.12.0/themes/base/jquery-ui.css">
<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
<script src="https://code.jquery.com/ui/1.12.0/jquery-ui.js"></script>
<link href="~/fonts/fontstyle.css" rel="stylesheet" />


<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header border-default">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">প্রশিক্ষণ এডিট</h4>
        </div>

        <div class="modal-body">

            @using (Html.BeginForm("Prassikkanedit", "Tranning", FormMethod.Post))
            {
                @Html.AntiForgeryToken()

                <div class="row">

                    @Html.ValidationSummary(true)

                    <div class="row">@Html.HiddenFor(model => model.Id)</div>
                    <div class="row">@Html.HiddenFor(model => model.MemberId)</div>
                    <div class="row">

                        <div class="col-xs-6 col-sm-6 col-md-6">
                            <div class="form-group">
                                <label class="col-md-3 control-label">সনদ নং</label>
                                <div class="col-md-9">
                                    @Html.TextBoxFor(model => model.SNo, new { @class = "form-control", required = "true" })
                                </div>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <input type="submit" id="btnedit" class="btn btn-info" value="Update" />
                        </div>

                    </div>
                    
                </div>
            }



        </div>

        <div class="modal-footer border-default">
            <button type="button" class="btn btn-link" data-dismiss="modal">Close</button>

        </div>
    </div>
</div>


